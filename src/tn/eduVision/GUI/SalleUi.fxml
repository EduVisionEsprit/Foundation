<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.eduVision.GUI.SalleController">
    <children>
        <VBox prefHeight="400.0" prefWidth="600.0">
            <TableView fx:id="salleTable">
                <columns>
                    <TableColumn fx:id="nomSalleColumn" prefWidth="200.0" text="Nom Salle" />
                    <TableColumn fx:id="capaciteSalleColumn" prefWidth="100.0" text="Capacite Salle" />
                    <TableColumn fx:id="typesalle" prefWidth="150.0" text="Type Salle" />
                </columns>
            </TableView>
            <Button text="Insert" onAction="#handleInsertButtonClicked" />
        </VBox>
    </children>
</AnchorPane>
