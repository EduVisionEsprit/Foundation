<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.eduVision.GUI.ReleverNote_Attestation_ElitesGUIController">


    <children>
