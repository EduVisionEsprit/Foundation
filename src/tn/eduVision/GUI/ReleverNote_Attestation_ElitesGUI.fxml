                <Label text="Session d'examen:" />
                <ComboBox fx:id="cmbSessionsExamen" prefWidth="200.0" />
            </HBox>
            <HBox spacing="10">
                <Label text="Matière:" />
                <ComboBox fx:id="cmbMatieres" prefWidth="200.0" />
